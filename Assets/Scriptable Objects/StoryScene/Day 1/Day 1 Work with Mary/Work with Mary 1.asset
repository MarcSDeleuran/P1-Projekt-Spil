    speaker: {fileID: 11400000, guid: 4e60b212002e57d4e98d751b81219055, type: 2}
    Actions: []
    Music: {fileID: 0}
    Sound: {fileID: 0}
    StressChange: 0
    AcademicChange: 0
    SocialChange: 0
