      MoveSpeed: 0
    Music: {fileID: 0}
    Sound: {fileID: 0}
    StressChange: 0
    AcademicChange: 0
    SocialChange: 0
  - text: "\u201CHey, We survived the lecture, Huh? I was about to fall asleep.\u201D
      She follows with a short laugh"
    speaker: {fileID: 11400000, guid: e724d5a98870e1249836d7b5c98969e0, type: 2}
    Actions:
    - Speaker: {fileID: 11400000, guid: e724d5a98870e1249836d7b5c98969e0, type: 2}
