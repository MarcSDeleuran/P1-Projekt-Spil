    Actions: []
    Music: {fileID: 0}
    Sound: {fileID: 0}
    StressChange: 0
    AcademicChange: 0
    SocialChange: 0
  - text: '"You are allowed to work with one partner. Good luck everyone!"'
    speaker: {fileID: 11400000, guid: 0ecb6b5de50549b4abd8948724a115ca, type: 2}
    Actions:
    - Speaker: {fileID: 11400000, guid: 0ecb6b5de50549b4abd8948724a115ca, type: 2}
